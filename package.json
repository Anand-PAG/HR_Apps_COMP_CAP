  "scripts": {
    "start": "cds-serve"
  },
  "cds": {
    "requires": {
      "[production]": {
        "auth": "xsuaa"
      },
      "[hybrid]": {
        "auth": "mocked"
      },
      "db": {
        "kind": "hana-cloud"
      },
      "connectivity": true,
      "destinations": true,
      "application-logging": true
    },
    "sql": {
      "native_hana_associations": false
    },
    "server": {
      "body_parser": {
        "limit": "20000mb"
      }
    },
    "query": {
      "limit": {
        "default": 1500000,
        "max": 1500000
      }
    }
  }
